    "mkdirp": "^0.5.1",
    "mock-nodecg": "^1.7.0",
    "ora": "^2.1.0",
    "p-each-series": "^1.0.0",
    "pixelmatch": "^4.0.2",
    "pngjs": "^3.3.3",
    "puppeteer": "^1.15.0",
    "tmp": "0.0.33",
    "tslib": "^1.9.1",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@types/cheerio": "^0.22.7",
    "@types/express": "^4.16.0",
    "@types/mkdirp": "^0.5.2",
